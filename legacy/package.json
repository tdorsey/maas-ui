{
  "name": "@maas-ui/legacy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "webpack -p --config webpack.config.js",
    "build-dev": "NODE_ENV=development webpack --config webpack.config.js",
    "clean": "rm -rf node_modules build",
    "clean-build": "rm -rf build",
    "lint": "yarn lint-package-json && yarn lint-js",
    "lint-js": "eslint ./src/app/",
    "lint-package-json": "npmPkgJsonLint .",
    "prettier": "prettier --write 'src/**/*.{js,scss}' '!**/build.scss' '!**/*-min.js'",
    "prettier-check": "prettier --check 'src/**/*.{js,scss}' '!**/build.scss' '!**/*-min.js'",
    "start": "webpack-dev-server --mode development --open --config webpack.config.js --port 8402 --host 0.0.0.0 --disable-host-check",
    "test": "jest",
    "watch": "NODE_ENV=development webpack --watch"
  },
  "devDependencies": {
    "@babel/cli": "7.8.3",
    "@babel/core": "7.8.3",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@babel/preset-react": "7.8.3",
    "@types/prop-types": "15.7.3",
    "angular-mocks": "1.7.9",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-angularjs-annotate": "0.10.0",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.4.1",
    "dotenv-flow-webpack": "1.0.0",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.4.3",
    "eslint": "6.8.0",
    "eslint-plugin-react": "7.17.0",
    "html-loader-jest": "0.2.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.9.0",
    "jest-fetch-mock": "3.0.1",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.13.0",
    "npm-package-json-lint": "4.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "prettier": "1.19.1",
    "prop-types": "15.7.2",
    "raw-loader": "4.0.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "sass-loader": "8.0.0",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1",
    "webpack-merge": "4.2.2"
  },
  "dependencies": {
    "@maas-ui/shared": "0.1.0",
    "angular": "1.7.9",
    "angular-cookies": "1.7.9",
    "angular-route": "1.7.9",
    "angular-sanitize": "1.7.9",
    "angular-vs-repeat": "1.1.11",
    "babel-polyfill": "6.26.0",
    "cross-env": "6.0.3",
    "jquery": "3.4.1",
    "js-cookie": "2.2.1",
    "macaroon-bakery": "0.2.1",
    "ng-tags-input": "3.2.0",
    "vanilla-framework": "2.0.1"
  },
  "npmpackagejsonlint": {
    "rules": {
      "prefer-absolute-version-dependencies": "error",
      "prefer-absolute-version-devDependencies": "error",
      "prefer-alphabetical-dependencies": "error",
      "prefer-alphabetical-devDependencies": "error"
    }
  },
  "resolutions": {
    "lodash": "4.17.15"
  }
}
